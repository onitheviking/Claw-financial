# Plaid API credentials â€” get yours at https://dashboard.plaid.com
PLAID_CLIENT_ID=your_client_id_here
PLAID_SECRET=your_secret_here

# Plaid environment: sandbox | production
# Use "sandbox" for testing with fake data, "production" for real bank connections
# Compatible with plaid-cli's PLAID_ENVIRONMENT var (both are supported)
PLAID_ENVIRONMENT=sandbox

# Optional: comma-separated country codes (default: US)
# PLAID_COUNTRIES=US,CA

# Optional: language (default: en)
# PLAID_LANGUAGE=en

# Local server port for the account management dashboard
CLAW_FINANCIAL_PORT=9876

# Encryption key for securing access tokens at rest (auto-generated during setup if blank)
CLAW_FINANCIAL_ENCRYPTION_KEY=
